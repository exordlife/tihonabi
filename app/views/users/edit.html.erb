<div class="jumbotron">
  <h4 class="display-5">ニックネームの変更</h4>
  
  <hr class="my-4">
  <%= form_with model: @user, url: user_path(@user) ,local:true do |f| %>
  <h4>ニックネーム</h4>
    <%= f.text_field :name %>
  <h4>ProfileImage</h4>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: ".jpg,.gif,.png,image/gif,image/jpeg,image/png" %> 
  <%= f.submit '更新' %>
<% end %>
<div class="mt-3"><%= link_to "戻る", user_path(current_user),class: "btn btn-outline-secondary" %></div>
</div>